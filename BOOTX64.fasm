format pe64 efi
entry Main

section '.text' code readable executable
include "UEFI/UEFI.fasm"

Main:
mov [UEFIHandle],rcx
mov [UEFISystemTable],rdx

UEFISimpleTextOutputProtocolClearScreen
UEFISimpleTextOutputProtocolOutputString Text

jmp $

section '.data' data readable writable
UEFIHandle:         dq ?
UEFISystemTable:    dq ?
Text:               du "Main Operating System will start",0
