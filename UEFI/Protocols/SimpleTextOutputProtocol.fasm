macro UEFISimpleTextOutputProtocolClearScreen
{
    sub rsp,0x20

    mov rdx,[UEFISystemTable]
    mov rcx,[rdx+SystemTable.ConOut]
    mov rax,[rcx+SimpleTextOutputProtocol.ClearScreen]
    call rax

    add rsp,0x20
}

macro UEFISimpleTextOutputProtocolOutputString String

{
    sub rsp,0x20

    mov rdx,[UEFISystemTable]
    mov rcx,[rdx+SystemTable.ConOut]
    mov rax,[rcx+SimpleTextOutputProtocol.OutputString]
    lea rdx,String
    call rax

    add rsp,0x20
}
